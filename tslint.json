{
  "extends": "tslint:recommended",
  "rules": {
	"no-consecutive-blank-lines": false,
    "no-console": [false, "log", "error"],
	"no-trailing-whitespace": false,
    "quotemark": [true, "single", "avoid-escape"],
    "prefer-const": false,
	"typedef-whitespace": [
      false,
      {},
      { "call-signature": "space" }
    ]
  }
}